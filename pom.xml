<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>qpe-mapview-rest</artifactId>
	<packaging>jar</packaging>
	<version>0.0.6-SNAPSHOT</version>
	<name>QPE classic REST mapview sample</name>
	<parent>
		<groupId>com.quuppa</groupId>
		<artifactId>quuppa-parent</artifactId>
		<version>1.0.2</version>
	</parent>
	
	<scm>
		<connection>scm:git:git@gitlab.com:quuppa/samples/qpe-mapview-rest.git</connection>
		<tag>HEAD</tag>
		<url>https://gitlab.com/quuppa/samples/qpe-mapview-rest.git</url>
	</scm>
	
	
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<archive>
						<addMavenDescriptor>false</addMavenDescriptor>
					</archive>
					<excludes>
						<exclude>lib/**</exclude>
					</excludes>					
				</configuration>
			</plugin>			
		</plugins>
	</build>
</project>
